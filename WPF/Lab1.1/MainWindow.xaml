<Window x:Class="Lab1._1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab1._1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button x:Name="button" Content="Set Name" HorizontalAlignment="Left" Margin="183,116,0,0" VerticalAlignment="Top" Height="73" Width="94" Click="Button_Click"/>
        <Button x:Name="button1" Content="Ret Name" HorizontalAlignment="Left" Margin="183,236,0,0" VerticalAlignment="Top" Height="85" Width="94" Click="Button_Click_1"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="400,116,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="276" Height="73" RenderTransformOrigin="0.489,0.527" TextChanged="textBox_TextChanged"/>
        <Label x:Name="label" HorizontalAlignment="Left" Margin="400,236,0,0" VerticalAlignment="Top" Height="85" Width="276"/>
        <Button Canvas.Left="10" Canvas.Top="10" Content="Click Me" Height="23" Name="button3" Width="75" Click="button3_Click"/>
        <TextBox Width="170" Canvas.Left="90" Canvas.Top="12" Name="MyTextBox" />

    </Canvas>
</Window>
